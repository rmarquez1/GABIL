 #! /bin/bash
 # Las líneas que empiezan por "#" son comentarios
 # La primera línea o #! /bin/bash asegura que se interpreta como
 # un script de bash, aunque se ejecute desde otro shell.

 # "python nombrePrograma nombreArchivo tasaMutacion tasaCrossover tipoSeleccion penalizacion"

echo "Tercer Script"

limite=10
i=0;
 
while [ $limite -ge $i ]
do
     echo Acción $i ejecutada
     python gabil.py iris.data 0.02 0.85 1 1
     let i=$i+1
done

echo "Fin del Tercer Script"